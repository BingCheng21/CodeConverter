{
    "name": "codeconverter",
    "version": "0.1.0",
    "private": true,
    "eslintConfig": {
        "extends": "react-app"
    },
    "scripts": {
        "start": "rimraf ./build && react-scripts start",
        "build": "react-scripts build",
        "test": "cross-env CI=true react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "lint": "eslint ./src/"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-react",
                {
                    "runtime": "automatic"
                }
            ]
        ]
    },
    "dependencies": {
        "@monaco-editor/react": "^4.3.1",
        "axios": "^0.24.0",
        "bootstrap": "^5.1.3",
        "monaco-editor": "^0.30.1",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-bootstrap": "^0.25.0",
        "react-router-dom": "^6.0.2",
        "reactstrap": "^9.0.1",
        "rimraf": "^2.6.2"
    },
    "devDependencies": {
        "@types/jest": "^27.0.2",
        "@types/react": "^17.0.35",
        "@types/react-dom": "^17.0.11",
        "ajv": "^8.8.1",
        "cross-env": "^7.0.3",
        "eslint-config-react-app": "^6.0.0",
        "jquery": "^3.6.0",
        "popper.js": "^1.16.1",
        "react-scripts": "^5.0.0-next.47",
        "typescript": "^4.5.2"
    }
